{
  "name": "reach",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext js,ts,tsx src/ webpack/",
    "prettier-check": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run lint && npm run prettier-check",
    "pre-push": "npm run build; npm run deploy",
    "dev": "webpack serve --config ./webpack/config.dev.ts",
    "build": "rm -rf build; webpack --progress --config ./webpack/config.prod.ts",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/node": "^18.18.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0"
  }
}
